          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH dernier_etat AS (\r\n    SELECT\r\n        fev.fk_aeronef,\r\n        da.pk_aeronef AS aeronef_id,\r\n        dp.latitude,\r\n        dp.longitude,\r\n        dpy.nom_pays AS pays,\r\n        fev.fk_temps AS dernier_temps,\r\n        COALESCE(fev.on_ground, false) AS on_ground,\r\n        fev.geo_altitude AS altitude_geo,\r\n        ROW_NUMBER() OVER (\r\n            PARTITION BY fev.fk_aeronef\r\n            ORDER BY fev.fk_temps DESC\r\n        ) AS rn\r\n    FROM fait_etat_vol fev\r\n    JOIN dim_aeronef da \r\n        ON fev.fk_aeronef = da.pk_aeronef\r\n    JOIN dim_position dp \r\n        ON fev.fk_position = dp.pk_position\r\n    JOIN dim_pays dpy \r\n        ON fev.fk_pays = dpy.pk_pays\r\n    WHERE $__timeFilter(fev.fk_temps)\r\n      AND dp.latitude IS NOT NULL\r\n      AND dp.longitude IS NOT NULL\r\n      AND dp.latitude <> 0\r\n      AND dp.longitude <> 0\r\n)\r\nSELECT\r\n    aeronef_id,\r\n    latitude,\r\n    longitude,\r\n    pays,\r\n    dernier_temps,\r\n    altitude_geo,\r\n    CASE \r\n        WHEN on_ground = true THEN 'au sol'\r\n        ELSE 'en vol'\r\n    END AS etat_sol\r\nFROM dernier_etat\r\nWHERE rn = 1;\r\n",
          "refId": "A",
